.weui-grid-dingdan {  
 height: 100rpx;
  box-sizing: border-box;  
  border: bottom 100px;
  margin: 10px;
  border-color:  #000000;
}  
.weui-grid-dingdan:before {  
 /* border-right: 1px solid #D9D9D9; */ 
 border-color:  #000000;
}  
.weui-grid-dingdan:after {   
  /*border-bottom: 1px solid #D9D9D9;  */
  border-color:  #000000;
}  
.naviegatorss{
  height: 100rpx;
  border:0 0 20rpx 0 #000000;
}
.weui-grid-dingdan:active {  
  background-color: #ECECEC;  
  border-color:  #000000;
}  
.weui-grid__icon-dingdan {  
  width: 64rpx;  
  height: 64rpx;  
  margin: 0 auto; 
  float: left; 
}  
.weui-grid__icon-dingdan image {  
  display: block;  
  width: 100%;  
  height: 100%;  
}  
.weui-grid__icon-dingdan + .weui-grid__label-dingdan {  
  margin-top: 10rpx;  
}  

.weui-grid__label-dingdan {  
  display: block;  

  color: #000000;  
  font-size: 30rpx;  
  white-space: nowrap;  
  text-overflow: ellipsis;  
  overflow: hidden; 
  position: relative;
  top: 20rpx; 
  left: 30rpx;
}  

/*服务栏css*/

.weui-grids-fuwu {  
  position: relative;  
  overflow: hidden;  
}  
.weui-grids-fuwu:before {  
  content: " ";  
  position: absolute;  
  left: 0;  
  top: 0;  
  right: 0;  
  height: 1px;  
  /*border-top: 1px solid #D9D9D9;*/  
  color: #D9D9D9;  
}  
.weui-grids-fuwu:after {  
  content: " ";  
  position: absolute;  
  left: 0;  
  top: 0;  
  width: 1px;  
  bottom: 0;  
  /*border-left: 1px solid #D9D9D9;*/  
  color: #D9D9D9;  
}  

.weui-grid-fuwu {  
  position: relative;  
  float: left;  
  padding: 40rpx 20rpx;  
  width: 25%;  
  box-sizing: border-box;  
}  
.weui-grid-fuwu:before {  
  content: " ";  
  position: absolute;  
  right: 0;  
  top: 0;  
  width: 1px;  
  bottom: 0;  
 /* border-right: 1px solid #D9D9D9; */ 
  color: #D9D9D9;  
}  
.weui-grid-fuwu:after {  
  content: " ";  
  position: absolute;  
  left: 0;  
  bottom: 0;  
  right: 0;  
  height: 1px;  
  /*border-bottom: 1px solid #D9D9D9;  */
  color: #D9D9D9;  
}  
.weui-grid-fuwu:active {  
  background-color: #ECECEC;  
}  
.weui-grid__icon-fuwu {  
  width: 64rpx;  
  height: 64rpx;  
  margin: 0 auto;  
}  
.weui-grid__icon-fuwu image {  
  display: block;  
  width: 100%;  
  height: 100%;  
}  
.weui-grid__icon-fuwu + .weui-grid__label-fuwu {  
  margin-top: 10rpx;  
}  
.weui-grid__label-fuwu {  
  display: block;  
  text-align: center;  
  font-weight: bold;  
  color: #000000;  
  font-size: 28rpx;  
  white-space: nowrap;  
  text-overflow: ellipsis;  
  overflow: hidden;  
} 


